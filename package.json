{
  "name": "react-performanalyzer",
  "version": "0.0.6",
  "description": "A comprehensive React performance analysis tool that checks for memory leaks, performance issues, and code quality problems",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "react-performanalyzer": "dist/cli.js",
    "rperf": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "build:prod": "tsup --minify",
    "dev": "tsup --watch",
    "start": "node dist/cli.js",
    "prepare": "npm run build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "performance",
    "analysis",
    "memory-leaks",
    "code-quality",
    "typescript",
    "javascript",
    "linting",
    "optimization"
  ],
  "author": "Mohammad Garmabi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohamadgarmabi/react-performanalyzer.git"
  },
  "bugs": {
    "url": "https://github.com/mohamadgarmabi/react-performanalyzer/issues"
  },
  "homepage": "https://github.com/mohamadgarmabi/react-performanalyzer#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^20.19.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}