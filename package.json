{
  "name": "react-perf",
  "version": "1.0.0",
  "description": "A comprehensive React performance analysis tool that checks for memory leaks, performance issues, and code quality problems",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "react-perf": "dist/cli.js",
    "rperf": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:optimized": "webpack --mode production",
    "build:analyze": "webpack --mode production --analyze",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test",
    "size": "npm run build:optimized && npm run analyze:size",
    "analyze:size": "npx webpack-bundle-analyzer dist/bundle-stats.json",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "optimize": "npm run build:optimized && npm run analyze:bundle"
  },
  "keywords": [
    "react",
    "performance",
    "analysis",
    "memory-leaks",
    "code-quality",
    "typescript",
    "javascript",
    "linting",
    "optimization"
  ],
  "author": "Mohammad Garmabi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohamadgarmabi/react-perf.git"
  },
  "bugs": {
    "url": "https://github.com/mohamadgarmabi/react-perf/issues"
  },
  "homepage": "https://github.com/mohamadgarmabi/react-perf#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^20.19.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "compression-webpack-plugin": "^10.0.0",
    "eslint": "^8.57.1",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}